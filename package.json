{
  "name": "task_management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite preview",
    "build": "tsc && vite build",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "rm:all": "rm -rf node_modules .next out dist build",
    "re:start": "pnpm rm:all && pnpm install && pnpm dev",
    "re:build": "pnpm rm:all && pnpm install && pnpm build",
    "re:build-npm": "pnpm run rm:all && pnpm install && pnpm run build",
    "dev:host": "vite --host"
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@reduxjs/toolkit": "^2.2.7",
    "antd": "^5.21.2",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "postcss-cli": "^11.0.0",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.2",
    "react-toastify": "^10.0.5",
    "redux-persist": "^6.0.0",
    "tailwindcss": "^3.4.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/lodash": "^4.17.9",
    "@types/react": "^18.3.3",
